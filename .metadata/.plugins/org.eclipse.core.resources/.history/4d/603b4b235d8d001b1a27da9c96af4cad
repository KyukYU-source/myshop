<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mem">
	<select id="select" parameterType="dto2" resultMap="resMap">
		select * from member where id = #{id}
	</select>
	<select id="selectAll" resultMap="resMap">
		select * from member
	</select>
	<resultMap type="dto2" id="resMap">
		<result property="id" column="id"/>
		<result property="pw" column="pw"/>
		<result property="name" column="name"/>
		<result property="tel" column="tel"/>
	</resultMap>
	
	<insert id="insert" parameterType="dto2">
		insert into member values (#{id}, #{pw}, #{name}, #{tel})
	</insert>
	<update id="update" parameterType="dto2">
		update member set tel = #{tel} where id = #{id}
	</update>	
	<delete id="delete" parameterType="dto2">
		delete from member where id = #{id}
	</delete>
</mapper>